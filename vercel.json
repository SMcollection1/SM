{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node@2.5.20" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "products/**/*.md", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
